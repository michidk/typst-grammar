  },
  "blockRawGeneral": {
   "name": "markup.raw.block.typst",
   "begin": "(`{3,})([\\p{XID_Start}_][\\p{XID_Continue}_\\-]*\\b)?",
   "beginCaptures": {
    "1": {
     "name": "punctuation.definition.raw.begin.typst"
    },
    "2": {
     "name": "fenced_code.block.language.typst"
