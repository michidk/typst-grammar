     "beginCaptures": {
      "0": {
       "name": "entity.name.type.primitive.hash.typst"
      }
     },
     "endCaptures": {
      "1": {
       "name": "punctuation.terminator.statement.typst"
      }
     },
     "patterns": [
      {
       "include": "#expression"
      }
     ]
    },
    {
     "begin": "#(?=(?:none)\\b(?!-))",
